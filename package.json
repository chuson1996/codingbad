    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npm install && bower install",
    "build:dev": "npm run clean && npm run prebuild && ./node_modules/webpack/bin/webpack.js --ENV=development",
    "build:staging": "npm run clean && npm run prebuild && ./node_modules/webpack/bin/webpack.js --ENV=staging",
    "build:production": "npm run clean && npm run prebuild && ./node_modules/webpack/bin/webpack.js --ENV=production",
    "serve:build": "grunt serve",
    "start": "npm run prebuild && node webpack.server.js",
    "clean": "rm -rf public && mkdir public",
    "deep-clean": "rm -rf bower_components && npm run clean"
